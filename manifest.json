{
  "manifest_version": 2,
  "name": "Test",
  "version": "1.0.6",
  "description": "This extension verifies the code exists somewhere on this page",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "content_security_policy": "script-src 'self' 'sha256-bHBOZA8vngL+AhN58HWCH5FySa496ifrvgj5EcrQZTo='"
}
